
Dim r As Long
For r = 13 To 12 + n
    If ws.Cells(r, "E").Value = 1 Then
        ws.Cells(r, "F").ClearContents
        ws.Cells(r, "F").Locked = True
    Else
        ws.Cells(r, "F").Locked = False
    End If
Next r

'Ensure protection so Locked cells behave
ws.Protect Password:="", UserInterfaceOnly:=True
