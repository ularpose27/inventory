# Cell 5 â€“ Read FX tab into a dataframe (values only)

fx_rates = pd.read_excel(
    file_path,
    sheet_name="FX",
    usecols="A:E",
    header=2    # Excel row 3
)

# Fill blank cells downward (typical for FX tables)
fx_rates = fx_rates.ffill()

print(fx_rates.head())
print("Rows loaded in fx_rates:", len(fx_rates))
