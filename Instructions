
With ws.Sort
    .SortFields.Clear

    'Sort on the ACTUAL data range, not header cell
    .SortFields.Add Key:=ws.Range("K2:K400"), SortOn:=xlSortOnValues, Order:=xlDescending, DataOption:=xlSortNormal
    .SortFields.Add Key:=ws.Range("I2:I400"), SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal

    .SetRange ws.Range("A1:K400")
    .Header = xlYes
    .MatchCase = False
    .Orientation = xlTopToBottom
    .SortMethod = xlPinYin
    .Apply
End With
