With securityWs.Sort
    .SortFields.Clear
    .SortFields.Add Key:=securityWs.Range("K2:K400"), SortOn:=xlSortOnValues, Order:=xlDescending, DataOption:=xlSortNormal
    .SortFields.Add Key:=securityWs.Range("I2:I400"), SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    .SetRange securityWs.Range("A1:K400")
    .Header = xlYes
    .MatchCase = False
    .Orientation = xlTopToBottom
    .SortMethod = xlPinYin
    .Apply
End With
