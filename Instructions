import pandas as pd

# Path to the previous file
previous_path = '/mnt/data/file-7O2LXvFX7Akml2VEnMAsrxwI'

# Read the "Trend" tab from previous_path into a DataFrame
df_previous = pd.read_excel(previous_path, sheet_name='Trend', header=None)

# Display the DataFrame to verify the extracted values
print(df_previous)

# You can also save the DataFrame to a new Excel file if needed
df_previous.to_excel('extracted_trend_values.xlsx', index=False, header=False)
