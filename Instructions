# Cell â€“ Reading Security tab (values only)

security = pd.read_excel(
    file_path,
    sheet_name="Security",
    usecols="I:AN",   # columns I to AN
    header=4          # row 5 in Excel (0-indexed)
)

# Convert empty strings or whitespace-only cells to NaN
security = security.replace(r"^\s*$", np.nan, regex=True)

print("security shape (rows, columns):", security.shape)
print(security.head())
