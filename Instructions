import pandas as pd

# Assuming 'final_step' is your DataFrame and 'As of Date' is the column with date information
# Convert the 'As of Date' column to datetime format if it's not already
final_step['As of Date'] = pd.to_datetime(final_step['As of Date'])

# Change the date format to 'DDMMAA'
final_step['As of Date'] = final_step['As of Date'].dt.strftime('%d%m%y')
