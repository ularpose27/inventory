import pandas as pd

# Path to the previous file
previous_path = '/mnt/data/file-7O2LXvFX7Akml2VEnMAsrxwI'

# Read the "Trend" tab from previous_path into a DataFrame
df_previous = pd.read_excel(previous_path, sheet_name='Trend', header=None)

# Find the last column index with a value in the first row
last_valid_index = df_previous.iloc[0].last_valid_index()

# Drop all columns after the last valid column
df_trimmed = df_previous.iloc[:, :last_valid_index + 1]

# Display the trimmed DataFrame to verify the changes
print(df_trimmed)

# Save the trimmed DataFrame to a new Excel file if needed
df_trimmed.to_excel('trimmed_trend_values.xlsx', index=False, header=False)
